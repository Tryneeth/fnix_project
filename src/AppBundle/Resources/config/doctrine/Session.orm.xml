<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\Session" table="session">
    <indexes>
      <index name="id_treatment" columns="id_treatment"/>
      <index name="id_admin" columns="id_admin"/>
    </indexes>
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="name" type="string" column="name" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="description" type="string" column="description" length="255" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="alarm" type="date" column="alarm" nullable="true"/>
    <field name="date" type="date" column="date" nullable="false"/>
    <many-to-one field="idTreatment" target-entity="Treatment" fetch="LAZY">
      <join-columns>
        <join-column name="id_treatment" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="idAdmin" target-entity="Admin" fetch="LAZY">
      <join-columns>
        <join-column name="id_admin" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
